{
  "name": "txt-to-sql",
  "description": "Tools for convert text data to SQL sentences",
  "version": "0.1.0",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "repository": "codenautas/txt-to-sql",
  "contributors": [{
    "name": "Emilio Platzer", "email": "emilioplatzer@gmail.com"
  },{
    "name": "Diego Florio", "email": "diegoefe@gmail.com"
  }],
  "license": "MIT",
  "main": "lib/txt-to-sql.js",
  "bin": {
    "txt-to-sql": "./bin/txt-to-sql-run.js"
  },
  "files": ["bin", "lib", "web"],
  "dependencies": {
    "commander": "2.11.0",
    "fs-promise": "~2.0.3",
    "iconv-lite": "~0.4.18",
    "js-yaml": "~3.9.1",

    "best-globals": "~0.9.14",
    "mini-tools": "~0.3.17"
  },
  "devDependencies": {
    "browserify": "~14.4.0",
    "buffer": "~5.0.7",
    "expect.js": "~0.3.1",
    "fs-extra": "~4.0.1",
    "istanbul": "~0.4.5",
    "mocha": "~3.5.0",
    "precommit-hook": "3.0.0",
    "pug": "~2.0.0-rc.3",
    "stylus": "~0.54.5",
    
    "discrepances": "~0.1.4",
    "js-to-html": "~0.9.3",
    "require-bro": "~0.1.5"
  },
  "engines": {
    "node": ">= 0.12"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-nc": "mocha --reporter spec --bail --check-leaks --no-colors test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "start": "node example/server.js",
    "qac": "qa-control . -v",
    "all": "npm test && npm run test-cov && npm run qac",
    "web": "node tools/web.js && git add tools && git add web",
    "lint": "jshint .",
    "validate": "npm ls"
  },
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 90,
    "run-in": "server",
    "stability": "extending",
    "type": "web"
  },
  "pre-commit": [
    "web"
  ]
}